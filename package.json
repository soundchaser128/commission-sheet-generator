{
  "name": "mitzi",
  "private": true,
  "description": "A web application for generating and downloading commission sheets for artists, writers etc.",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve build",
    "typecheck": "tsc -b"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.6.6",
    "@remix-run/node": "^1.6.4",
    "@remix-run/react": "^1.6.4",
    "@remix-run/serve": "^1.6.4",
    "@remix-run/server-runtime": "^1.6.4",
    "@remix-run/vercel": "^1.6.4",
    "clsx": "^1.2.1",
    "fraction.js": "^4.2.0",
    "html-to-image": "^1.9.0",
    "immer": "^9.0.15",
    "nanoid": "^3.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.2",
    "react-image-crop": "^10.0.4",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@remix-run/dev": "^1.6.4",
    "@remix-run/eslint-config": "^1.6.4",
    "@tailwindcss/forms": "^0.5.2",
    "@types/eslint": "^8.4.5",
    "@types/node": "^17.0.39",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "tailwindcss": "^3.1.5",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.13",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "engines": {
    "node": ">=14"
  }
}
